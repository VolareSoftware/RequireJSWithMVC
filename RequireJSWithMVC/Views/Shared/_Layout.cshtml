<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Adding RequireJS to an ASP.NET MVC project</title>
    <link href="~/Styles/lib/bootstrap.css" rel="stylesheet" />
    <link href="~/Styles/lib/bootstrap-theme.css" rel="stylesheet" />
    <link href="~/Styles/app/Site.css" rel="stylesheet" />
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("RequireJSWithMVC", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Page 1", "Index", "Home")</li>
                    <li>@Html.ActionLink("Page 2", "About", "Home")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container">
        @RenderBody()
    </div>

    <script src="~/Scripts/lib/jquery-2.1.1.js"></script>
    <script src="~/Scripts/lib/bootstrap.js"></script>
    <script src="~/Scripts/lib/moment.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
